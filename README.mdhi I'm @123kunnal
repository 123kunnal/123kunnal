#include <studio.h>
int main(){         //program to find greatest of
int n1,n2,n3;              3 numbers
printf("enter numbers ");
scanf("%d%d%d",&n1,&n2,&n3);
if(n1>n2&&n1>n3){
printf("%d is greatest",n1);}
else if(n2>n1&&n2>n3){
printf("%d is greatest",n2);}
else{
printf("%d is greatest",n3);}
return 0;}
